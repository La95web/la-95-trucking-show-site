---
export const prerender = false;

import SpanishLayout from "../layouts/SpanishLayout.astro";
import HeaderShop from "../components/HeaderShop.astro";
import ShopA from "../assets/image/movilshop.png";
import ShopB from "../assets/image/headerShopDesk.webp";
import Footer from "../components/Footer.astro";
import ShopItems from "../components/ShopItems.astro";
import GalleryCart from "../components/GalleryCart.astro";
---

<SpanishLayout title="la 95 trucking show">
  <HeaderShop
  link1="/carrito/"
  titleA="Tienda"
  imgA={ShopA}
  imgB={ShopB}
  />
  <GalleryCart
  title="artículos destacados"
  paragraph="lorem ipsum dolar sit amet"
  primarybutton="agregar"
  secondarybutton="ver más" />
  <ShopItems 
  title="articulos"
  paragraph="lorem ipsum dolar sit amet"
  primarybutton="agregar"
  />
  <Footer />
</SpanishLayout>
<script>
  import "../scripts/mainMenu.js";
  import "../scripts/shopItems.js";
</script>