---
import SpanishLayout from "../layouts/SpanishLayout.astro";
import Footer from "../components/Footer.astro";
import Realstate from "../components/Realstate.astro";
import room1 from "../assets/image/room1.png";
import room2 from "../assets/image/bad1.png";
import ModalRealstate from "../components/modalRealstate.astro";
---

<SpanishLayout title="Rentas - La 95 Trucking Show">

  <Realstate images={[room1.src, room2.src]} 
  title="Habitaciones en renta"
  paragraph1="$800/mo"
  paragraph2="1bd 1ba"
  paragraph3="Wifi"
  paragraph4="Estacionamiento"
  paragraph5="Amoblado"
  paragraph6="Habitación soleada en apartamento compartido Habitación soleada en apartamento compartido"
  paragraph7="San francisco, CA"
  button="contactar"
  />
  <ModalRealstate />
  <Footer />
</SpanishLayout>
<script>
  import "../scripts/mainMenu.js"
  import "../scripts/modal.js"
  import "../scripts/saveData/Realstate.js"
</script>